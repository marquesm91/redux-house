{
  "name": "redux-house",
  "version": "0.0.1",
  "description": "Redux House gives you a productive way to create Actions, Requests and Reducers",
  "main": "lib/index.js",
  "unpkg": "dist/redux-house.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "es",
    "lib",
    "dist",
    "src"
  ],
  "keywords": [
    "redux-house",
    "redux",
    "fp"
  ],
  "author": "Matheus Marques <marquesm91@gmail.com>",
  "homepage": "https://github.com/marquesm91/redux-house",
  "repository": {
    "type": "git",
    "url": "https://github.com/marquesm91/redux-house.git"
  },
  "bugs": {
    "url": "https://github.com/marquesm91/redux-house/issues"
  },
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.{js,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,md}\"",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "rollup -c && es-check es5 dist/redux-house.min.js",
    "build": "run-s lint test clean && run-p build:**",
    "test": "jest --coverage",
    "test:watch": "jest --watch --verbose",
    "clean": "rimraf coverage dist es lib",
    "lint": "xo",
    "prepublishOnly": "run-s build"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "es-check": "^5.0.0",
    "eslint": "^5.12.0",
    "eslint-config-jest-files": "^0.1.3",
    "eslint-config-unicorn-camelcase": "^0.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.2",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "semi": true,
    "eslintIntegration": true,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "unicorn-camelcase",
      "jest-files"
    ],
    "ignores": [
      "rollup.config.js",
      "jest1.config.js",
      "babel.config.js"
    ]
  }
}
